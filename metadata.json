{
    "name": "ajsmith-graphite",
    "version": "0.4.0",
    "author": "Alex Smith <alex.smith@redhat.com>",
    "summary": "Graphite server configuration using Docker.",
    "license": "GPL-2.0",
    "source": "https://github.com/ajsmith/puppet-graphite.git",
    "project_page": "https://github.com/ajsmith/puppet-graphite",
